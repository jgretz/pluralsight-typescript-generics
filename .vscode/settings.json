{
  "git.ignoreLimitWarning": true,
  "git.decorations.enabled": false,

  "files.autoSave": "afterDelay",

  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/.gitignore": true,
    "npm-debug.log": true,
    "*.js": true
  },

  "problems.decorations.enabled": true,

  "window.zoomLevel": 0,

  "terminal.integrated.fontSize": 18,

  "editor.cursorBlinking": "solid"
}
